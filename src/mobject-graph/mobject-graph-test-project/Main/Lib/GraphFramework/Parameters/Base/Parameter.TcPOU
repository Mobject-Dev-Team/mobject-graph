    <Method Name="Configure" Id="{c33d1ea8-59c8-0684-3a17-798cf0fc3705}">
      <Declaration><![CDATA[METHOD PUBLIC Configure
VAR_INPUT
	Settings : I_ParameterSettings;
END_VAR]]></Declaration>
      <Implementation>
